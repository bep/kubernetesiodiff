            &lt;<span style="color:#309;font-weight:bold">span</span> <span style="color:#309">style</span><span style="color:#555">=</span><span style="color:#c30">&#34;color:#0b0;font-weight:bold&#34;</span>&gt;var&lt;/<span style="color:#309;font-weight:bold">span</span>&gt; config = KubernetesClientConfiguration.BuildDefaultConfig();
