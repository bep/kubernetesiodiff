&lt;<span style="color:#309;font-weight:bold">div</span> <span style="color:#309">id</span><span style="color:#555">=</span><span style="color:#c30">&#34;kubectl-patch-example-1&#34;</span>&gt;
&lt;<span style="color:#309;font-weight:bold">pre</span>&gt;&lt;<span style="color:#309;font-weight:bold">code</span>&gt;    <span style="color:#999;font-weight:bold">&amp;lt;</span>div class=<span style="color:#999;font-weight:bold">&amp;quot;</span>highlight<span style="color:#999;font-weight:bold">&amp;quot;</span><span style="color:#999;font-weight:bold">&amp;gt;</span><span style="color:#999;font-weight:bold">&amp;lt;</span>pre style=<span style="color:#999;font-weight:bold">&amp;quot;</span>background-color:#f8f8f8;-moz-tab-size:4;-o-tab-size:4;tab-size:4<span style="color:#999;font-weight:bold">&amp;quot;</span><span style="color:#999;font-weight:bold">&amp;gt;</span><span style="color:#999;font-weight:bold">&amp;lt;</span>code class=<span style="color:#999;font-weight:bold">&amp;quot;</span>language-posh<span style="color:#999;font-weight:bold">&amp;quot;</span> data-lang=<span style="color:#999;font-weight:bold">&amp;quot;</span>posh<span style="color:#999;font-weight:bold">&amp;quot;</span><span style="color:#999;font-weight:bold">&amp;gt;</span>&lt;/<span style="color:#309;font-weight:bold">code</span>&gt;&lt;/<span style="color:#309;font-weight:bold">pre</span>&gt;
&lt;<span style="color:#309;font-weight:bold">p</span>&gt;
kubectl patch deployment patch-demo <span style="color:#999;font-weight:bold">&amp;ndash;</span>patch $(
