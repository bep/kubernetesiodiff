&lt;<span style="color:#309;font-weight:bold">pre</span>&gt;&lt;<span style="color:#309;font-weight:bold">code</span>&gt;    <span style="color:#999;font-weight:bold">&amp;lt;</span>div class=<span style="color:#999;font-weight:bold">&amp;quot;</span>highlight<span style="color:#999;font-weight:bold">&amp;quot;</span><span style="color:#999;font-weight:bold">&amp;gt;</span><span style="color:#999;font-weight:bold">&amp;lt;</span>pre style=<span style="color:#999;font-weight:bold">&amp;quot;</span>background-color:#f8f8f8;-moz-tab-size:4;-o-tab-size:4;tab-size:4<span style="color:#999;font-weight:bold">&amp;quot;</span><span style="color:#999;font-weight:bold">&amp;gt;</span><span style="color:#999;font-weight:bold">&amp;lt;</span>code class=<span style="color:#999;font-weight:bold">&amp;quot;</span>language-bash<span style="color:#999;font-weight:bold">&amp;quot;</span> data-lang=<span style="color:#999;font-weight:bold">&amp;quot;</span>bash<span style="color:#999;font-weight:bold">&amp;quot;</span><span style="color:#999;font-weight:bold">&amp;gt;</span>&lt;/<span style="color:#309;font-weight:bold">code</span>&gt;&lt;/<span style="color:#309;font-weight:bold">pre</span>&gt;
