&lt;<span style="color:#309;font-weight:bold">span</span> <span style="color:#309">style</span><span style="color:#555">=</span><span style="color:#c30">&#34;color:#080;font-style:italic&#34;</span>&gt;# Create a base/kustomization.yaml&lt;/<span style="color:#309;font-weight:bold">span</span>&gt;
