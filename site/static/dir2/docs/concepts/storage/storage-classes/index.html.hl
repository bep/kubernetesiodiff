    &lt;<span style="color:#309;font-weight:bold">tr</span>&gt;
      &lt;<span style="color:#309;font-weight:bold">td</span> <span style="color:#309">align</span><span style="color:#555">=</span><span style="color:#c30">&#34;left&#34;</span>&gt;
        CSI
      &lt;/<span style="color:#309;font-weight:bold">td</span>&gt;
      &lt;<span style="color:#309;font-weight:bold">td</span> <span style="color:#309">align</span><span style="color:#555">=</span><span style="color:#c30">&#34;left&#34;</span>&gt;
        1.14 (alpha), 1.16 (beta)
      &lt;/<span style="color:#309;font-weight:bold">td</span>&gt;
