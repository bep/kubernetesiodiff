    &lt;<span style="color:#309;font-weight:bold">p</span>&gt;
      The A records in
      &lt;<span style="color:#309;font-weight:bold">a</span> <span style="color:#309">href</span><span style="color:#555">=</span><span style="color:#c30">&#34;/docs/concepts/services-networking/dns-pod-service/&#34;</span>&gt;
