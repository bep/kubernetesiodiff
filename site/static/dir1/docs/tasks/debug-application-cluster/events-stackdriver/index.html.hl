jsonPayload.source.component=<span style="color:#999;font-weight:bold">&amp;#34;</span>default-scheduler<span style="color:#999;font-weight:bold">&amp;#34;</span>
jsonPayload.involvedObject.name:<span style="color:#999;font-weight:bold">&amp;#34;</span>nginx-deployment<span style="color:#999;font-weight:bold">&amp;#34;</span>&lt;/<span style="color:#309;font-weight:bold">code</span>&gt;&lt;/<span style="color:#309;font-weight:bold">pre</span>&gt;
